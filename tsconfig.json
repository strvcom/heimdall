{
  "compilerOptions": {
    "incremental": true,
    "noEmitOnError": true,
    "tsBuildInfoFile": ".buildstate/tsconfig.tsbuildinfo",
    "target": "ES2022",
    "module": "NodeNext",
    "declaration": true,
    "declarationMap": true,
    "inlineSourceMap": true,
    "importHelpers": true,
    "strict": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true
  },
  "typedocOptions": {
    "entryPoints": [
      "src/index.ts"
    ],
    "out": "docs",
    "includeVersion": true,
    "readme": "none"
  }
}
